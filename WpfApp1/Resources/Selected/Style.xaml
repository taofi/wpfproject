<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Label">
        <Setter Property="FontSize" Value="15"/>
        <Setter Property="FontFamily" Value="pack://application:,,,/WpfApp1;component/Resources/Fonts/#PT Sans Narrow Bold"/>
    </Style>
    <Style TargetType="Button">
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="FontFamily" Value="pack://application:,,,/WpfApp1;component/Resources/Fonts/#PT Sans Narrow Bold"/>
        <Setter Property="Background" Value="#CEBEED"/>
        <Setter Property="Height" Value="30"/>
    </Style>
    <Style TargetType="TextBox">
        <Setter Property="FontSize" Value="15"/>
        <Setter Property="FontFamily" Value="pack://application:,,,/WpfApp1;component/Resources/Fonts/#PT Sans Narrow Regular"/>
    </Style>
    <Style TargetType="Border">
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="CornerRadius" Value="10"/>
    </Style>
    <Style TargetType="Window">
        <Setter Property="Background" Value="#D2CFE1"/>
    </Style>
    <SolidColorBrush x:Key="MainBackground" Color="#D2CFE1"/>
    <SolidColorBrush x:Key="SecondBackground" Color="#E9EBF8"/>
</ResourceDictionary>